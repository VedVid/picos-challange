--player

function new_player(x, y, lvl)
  local player = {}
  player.x = x
  player.y = y
  player.lvl = lvl
  player.spr_idle = 1
  player.spr_left = 2
  player.spr_right = 3
  player.spr_up = 4
  player.spr_down = 5
  player.spr_current = 1
  return player
end
